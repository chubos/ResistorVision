{
  "expo": {
    "name": "Resistor Vision",
    "slug": "resistorvision",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "resistorvision",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff9800"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chubos.resistorvision",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "Resistor Vision needs camera access to recognize resistor band colors.",
        "UIRequiresFullScreen": false,
        "UIStatusBarHidden": false,
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ff9800",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "splash": {
        "backgroundColor": "#ff9800",
        "enableForceDarkMode": false
      },
      "package": "com.chubos.resistorvision",
      "versionCode": 3,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#ff9800",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#ff9800"
          }
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "ResistorVision needs camera access to recognize resistor band colors.",
          "enableMicrophonePermission": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "500eb1dc-b562-48f4-a132-cb1eb852439b"
      },
      "router": {}
    },
    "owner": "chubos"
  }
}
