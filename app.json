{
  "expo": {
    "name": "ResistorVision",
    "slug": "resistorvision",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "resistorvision",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#c57500"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chubos.resistorvision",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "ResistorVision potrzebuje dostępu do kamery, aby rozpoznawać kolory pasków rezystorów.",
        "UIRequiresFullScreen": false,
        "UIStatusBarHidden": false
      },
      "locales": {
        "pl": {
          "NSCameraUsageDescription": "ResistorVision potrzebuje dostępu do kamery, aby rozpoznawać kolory pasków rezystorów."
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#c57500",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "package": "com.chubos.resistorvision",
      "versionCode": 1,
      "screenOrientation": "portrait",
      "resizeableActivity": true,
      "supportsPictureInPicture": false,
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#c57500",
          "dark": {
            "image": "./assets/images/icon.png",
            "backgroundColor": "#c57500"
          }
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "ResistorVision potrzebuje dostępu do kamery, aby rozpoznawać kolory pasków rezystorów.",
          "enableMicrophonePermission": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "autolinking": {
      "exclude": [
        "expo-camera"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "500eb1dc-b562-48f4-a132-cb1eb852439b"
      },
      "router": {}
    },
    "owner": "chubos"
  }
}
